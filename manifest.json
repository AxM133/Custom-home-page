{
  "manifest_version": 3,
  "name": "Custom New Tab",
  "version": "1.0",
  "description": "Замена начальной вкладки на кастомизированную страницу.",
  "chrome_url_overrides": {
      "newtab": "index.html"
  },
  "icons": {
      "16": "icons/settings-icon.png",
      "48": "icons/settings-icon.png",
      "128": "icons/settings-icon.png"
  },
  "permissions": [
      "storage"
  ]
}
